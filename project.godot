; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[accessibility]

general/accessibility_support=2

[application]

config/name="Advanced Import Plugin"
config/description="A Godot 4.6+ plugin adding some bulk model import options."
config/version="0.2.1"
config/tags=PackedStringArray("gd_script", "github", "plugin")
config/features=PackedStringArray("4.6", "Forward Plus")
config/icon="uid://bsnkfithuowdm"

[debug]

gdscript/warnings/directory_rules={
"res://addons": 0,
"res://addons/advanced_model_import": 1
}
gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_call_argument=1

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[editor_plugins]

enabled=PackedStringArray("res://addons/GDQuest_GDScript_formatter/plugin.cfg", "res://addons/advanced_model_import/plugin.cfg", "res://addons/script-ide/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://addons/advanced_model_import/": "yellow",
"res://images/": "gray",
"res://test_data/": "gray"
}

[navigation]

2d/navigation_engine="Dummy"
3d/navigation_engine="Dummy"

[physics]

2d/physics_engine="Dummy"
3d/physics_engine="Dummy"

[rendering]

rendering_device/driver.windows="d3d12"
